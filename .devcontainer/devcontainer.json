{
  "name": "Phoenix Swap (Anchor v0.29.0)",
  "image": "ghcr.io/project-serum/anchor-dev:v0.29.0",
  "features": {
    "ghcr.io/devcontainers/features/node:1": {
      "version": "18"
    }
  },
  "postCreateCommand": "echo 'Running npm install'; npm install",
  "postAttachCommand": "source /etc/profile && echo 'Setting ANCHOR_WALLET'; export ANCHOR_WALLET=~/.config/solana/id.json",
  "customizations": {
    "vscode": {
      "extensions": [
        "rust-lang.rust-analyzer"
      ]
    }
  }
}
